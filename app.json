{
  "expo": {
    "name": "eazeya",
    "slug": "eazeya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "eazeya",
    "owner": "eazeya",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo/Eazeya.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eazeya.eazeya-mobile-app",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "",
        "NSLocationAlwaysUsageDescription": ""
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.eazeya.eazeya",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CALL_PHONE","ACCESS_BACKGROUND_LOCATION","ACCESS_FINE_LOCATION","CAMERA"]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
